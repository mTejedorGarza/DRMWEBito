        });



        function initiateUIControls() {
            $('.gridDatePicker').datepicker({
                todayBtn: "linked",
                keyboardNavigation: false,
                forceParse: false,
                calendarWeeks: true,
                autoclose: true,
                format: "@ConfigurationProperty.DateFormatDatePicker"
            });
            //Time Picker
            $('.gridTimePicker').clockpicker();

        }
		
		function SendSpartan_Traduction_Process_WorkflowData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateSpartan_Traduction_Process_Workflow").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateSpartan_Traduction_Process_Workflow")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});

            var isNew = $('#hdnIsNew').val();
           
            if (checkClientValidate('Spartan_Traduction_Process_WorkflowCreateForm')) {
                $.ajax({
                    url: '@Url.Content("~/Frontal/Spartan_Traduction_Process_Workflow/Post?IsNew=")' + isNew,
                    type: 'POST',
                    data: formData,
                    async: true,
                    success: function (result) {
                        if (result < 0) {
                            showNotification("@Resources.Resources.ErrorSave", "error");
                        } else {

                            showNotification("@Resources.Resources.SaveSuccess", "success");
                            callBack(result);
                        }
                    },
                    error: function (result) {
                        showNotification("@Resources.Resources.ErrorContactingServer", "error");
                    },
                    cache: false,
                    contentType: false,
                    processData: false
                });
            }
            return false;
        };
		
        function GetPropertyBag() {
            $.get('@Url.Action("Spartan_Traduction_Process_WorkflowPropertyBag", "Spartan_Traduction_Process_Workflow")', function (data) {
                $(".PropertyModal").html(data);
            });
        }
        function Spartan_Traduction_Process_WorkflowGetBusinessRule(FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null)
			{
				$('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldId').val(FieldId);
				$('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldName').val(FieldName);
				$('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("BusinessRule", "BussinessRule")', { FieldId: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldId').val(), FieldName: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldName').val(), ObjectId: '35190', Attribute: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleAttribute').val(), Screen: 'Spartan_Traduction_Process_Workflow' }, function (data) {
                $(".Spartan_Traduction_Process_WorkflowBusinessRuleModal").html(data);
            });
        }
        function Spartan_Traduction_Process_WorkflowGetAddBusinessRule(RuleId, FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null) {
				$('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldId').val(FieldId);
				$('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldName').val(FieldName);
				$('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("AddBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldId').val(), FieldName: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldName').val(), ObjectId: '35190', Attribute: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleAttribute').val(), Screen: 'Spartan_Traduction_Process_Workflow' }, function (data) {
                $(".Spartan_Traduction_Process_WorkflowAddBusinessRuleModal").html(data);
                $(".datatable").DataTable();
            });
        }
		function RemoveBusinessRule(RuleId) {
			$.get('@Url.Action("RemoveBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldId').val(), FieldName: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleFieldName').val(), ObjectId: '35190', Attribute: $('#hdnSpartan_Traduction_Process_WorkflowBusinessRuleAttribute').val(), Screen: 'Spartan_Traduction_Process_Workflow' }, function (data) {
				$(".Spartan_Traduction_Process_WorkflowBusinessRuleModal").html(data);
			});
		}

