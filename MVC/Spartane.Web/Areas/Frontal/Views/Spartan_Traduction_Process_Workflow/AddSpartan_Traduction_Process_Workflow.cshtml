    <script type="text/javascript">
        



        $(document).ready(function () {
			var currentIndex = @ViewBag.currentRowIndex

            var Spartan_Traduction_Process_WorkflowRowElement = "Spartan_Traduction_Process_WorkflowPop_" + currentIndex.toString();
            var abc = "if(dynamicFieldValidation('" + Spartan_Traduction_Process_WorkflowRowElement + "')){ Spartan_Traduction_Process_WorkflowInsertRowFromPopup(" + currentIndex + "); }";
            var insertRowHTML = '<a class="btn btn-primary m-b" onclick="' + abc + '">Save</a>';
            $('#dvSaveJob').html(insertRowHTML);
			


            initiateUIControls();
            EjecutarValidacionesAlComienzo();
