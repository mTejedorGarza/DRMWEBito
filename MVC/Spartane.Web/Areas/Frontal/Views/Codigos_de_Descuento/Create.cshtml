                    $('#Fecha_de_Registro').val(result.data.Fecha_de_Registro);
                    $('#Hora_de_Registro').val(result.data.Hora_de_Registro);
                    $('#Usuario_que_Registra').val(result.data.Usuario_que_Registra);
                    $('#Tipo_de_Vendedor').val(result.data.Tipo_de_Vendedor);
                    $('#Vendedor').val(result.data.Vendedor);
                    $('#Tipo_de_Descuento').val(result.data.Tipo_de_Descuento);
                    $('#Descuento').val(result.data.Descuento);
                    getMS_Planes_por_Codigo_de_DescuentoData();
                    $('#Texto_promocional').val(result.data.Texto_promocional);
                    $('#Fecha_inicio_vigencia').val(result.data.Fecha_inicio_vigencia);
                    $('#Fecha_fin_vigencia').val(result.data.Fecha_fin_vigencia);
                    $('#Cantidad_maxima_de_referenciados').val(result.data.Cantidad_maxima_de_referenciados);
                    $('#Codigo_de_descuento').val(result.data.Codigo_de_descuento);
                    $('#Estatus').val(result.data.Estatus);

