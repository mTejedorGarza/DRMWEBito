        if ($('.Original_Text' + 'Header').css('display') != 'none') columnData[counter++] = $($($.parseHTML(inputData)).val(data.Original_Text)).addClass('Original_Text').attr('id', nameOfTable + 'Original_Text' + rowIndex).attr('data-field', 'Original_Text');
