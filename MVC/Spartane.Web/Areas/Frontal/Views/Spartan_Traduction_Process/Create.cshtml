                Spartan_Traduction_Process_DataTable.fnDeleteRow(rowIndexTable );
                showSpartan_Traduction_Process_DataGrid(Spartan_Traduction_Process_DataTable.fnGetData());
            }
        }
    }

    function Spartan_Traduction_Process_DatarowCreationGrid(row, data, index) {
        var permission =  {
            Edit: '@permissionSpartan_Traduction_Process_Data.Edit' == 'True',
            Delete: '@permissionSpartan_Traduction_Process_Data.Delete' == 'True'
        };
        setSpartan_Traduction_Process_DataValidation();
        var Spartan_Traduction_Process_DataRowElement = "Spartan_Traduction_Process_Data_" + index.toString();
        $(row).addClass(Spartan_Traduction_Process_DataRowElement);
        var nameOfTable='Spartan_Traduction_Process_Data_';
        var rowIndex = '_' + index;
        Spartan_Traduction_Process_DatainsertRowCurrentIndex = index;

        row.innerHTML = "";

        if (data.IsInsertRow) {

            if (row.childNodes.length != 0) {
                for (var i = 0; i < row.childNodes.length; i++) {
                    row.childNodes[i].remove();
                }
            }
            var controls = GetInsertSpartan_Traduction_Process_DataRowControls(index);
            var actionColInsert = $('<td>');

            var abc = "if(dynamicFieldValidation('" + Spartan_Traduction_Process_DataRowElement + "')){ Spartan_Traduction_Process_DataInsertRow(" + index + "); }";
            var insertRowHTML = '<a  onclick="' + abc + '">';
            $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
            $('<i class="fa fa-times">').appendTo($("<a  onclick='Spartan_Traduction_Process_DataCancelRow(" + index + ")'>").appendTo(actionColInsert));
            actionColInsert.appendTo(row);

            for (i = 0; i < controls.length; i++) {
                var idHeader = $(controls[i]).data('field') + 'Header';
                if($(controls[i]).length > 1)
                {
                    idHeader = $($(controls[i])[1]).data('field') + 'Header';
                }
                if($('#Spartan_Traduction_Process_DataGrid .' + idHeader ).css('display') != 'none')
                {
                    $(controls[i]).appendTo($('<td>').appendTo(row));
                }
            }
            $('#Spartan_Traduction_Process_DataGrid .inputMoney').inputmask("currency", { prefix: "", rightAlign: false });
            return;
        }

        var actionCol = $('<td>');
        var edit = "<a title='Edit' class='edit-MR' onclick='Spartan_Traduction_Process_DataEditRow(" + index + ", this)'>";
        if (!permission.Edit || isConsultMode)
            edit = "<a title='Edit' class='not-permission edit-MR' data-placement='top' data-toggle='tooltip' href='#' data-original-title='Usted no tiene permisos para realizar esta acción'>";
        var editPopUp = "<a title='Edit' class='edit-popup-MR' data-toggle='modal' data-target='#AddSpartan_Traduction_Process_Data-form' onclick='Spartan_Traduction_Process_DataEditRowPopup(" + index + ", this)'>";
        if (!permission.Edit || isConsultMode)
            editPopUp = "<a title='Edit' class='not-permission edit-popup-MR' data-placement='top' data-toggle='tooltip' href='#' data-original-title='Usted no tiene permisos para realizar esta acción'>";
        var eliminar = "<a title='Delete' class='eliminar-MR' onclick='Spartan_Traduction_Process_DataDeleteRow(" + index + ", this)'>";
        if (!permission.Delete || isConsultMode)
            eliminar = "<a title='Delete' class='not-permission eliminar-MR' data-placement='top' data-toggle='tooltip' href='#' data-original-title='Usted no tiene permisos para realizar esta acción'>";
        $('<i class="fa fa-pencil">').appendTo($(edit).appendTo(actionCol));
        $('<i class="fa fa-edit">').appendTo($(editPopUp).appendTo(actionCol));
        $('<i class="fa fa-trash-o">').appendTo($(eliminar).appendTo(actionCol));
        if ($('.Action' + 'Header').css('display') != 'none')
            actionCol.appendTo(row);
        if ($('.Concept' + 'Header').css('display') != 'none')
            $('<td id="'+nameOfTable+'Concept'+rowIndex+'">').text(GetSpartan_Traduction_Process_Data_Spartan_Traduction_Concept_TypeName(data.Concept)).appendTo(row);
        if ($('.Name_of_Control' + 'Header').css('display') != 'none')
            $('<td id="'+nameOfTable+'Name_of_Control'+rowIndex+'">').text(data.Name_of_Control).appendTo(row);
        if ($('.Original_Text' + 'Header').css('display') != 'none')
            $('<td id="'+nameOfTable+'Original_Text'+rowIndex+'">').text(data.Original_Text).appendTo(row);
        if ($('.Translated_Text' + 'Header').css('display') != 'none')
            $('<td id="'+nameOfTable+'Translated_Text'+rowIndex+'">').text(data.Translated_Text).appendTo(row);

    }

