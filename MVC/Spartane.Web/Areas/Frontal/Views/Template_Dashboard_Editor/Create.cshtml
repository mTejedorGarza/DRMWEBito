        });

        var Template_Dashboard_DetailinsertRowCurrentIndex = -1;
        var Template_Dashboard_DetailDataDataTable;
        function getTemplate_Dashboard_DetailData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/Template_Dashboard_Editor/GetTemplate_Dashboard_Detail")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceTemplate_Id").val() != "" ? "&RelationId=" + $("#ReferenceTemplate_Id").val() : ""),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                    Template_Dashboard_DetailDataDataTable = result.data;
                    showTemplate_Dashboard_DetailGrid(result.data);
                    $('#Template_Dashboard_DetailGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
        function Template_Dashboard_DetailGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";
            columnData[counter++] = $($($.parseHTML(inputData)).val(data.Row_Number)).addClass('Row_Number inputNumber').attr('id', nameOfTable + 'Row_Number' + rowIndex).attr('data-field', 'Row_Number');
            columnData[counter++] = $($($.parseHTML(inputData)).val(data.Columns)).addClass('Columns inputNumber').attr('id', nameOfTable + 'Columns' + rowIndex).attr('data-field', 'Columns');

            return columnData;
        }



        function GetFileInfo(fileData, fileUploadData) {

            var $div = $("<div>");
            if (fileUploadData != null && fileUploadData.files.length > 0) {

                $div.append("<label>" + fileUploadData.files[0].name + "</label>");
                //<i class="fa fa-times-circle" id='removeFileAttachment'>
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;

            } else if (fileUploadData != null) {
                return $.parseHTML(GetFileUploader());
            } else {
                $div.append("<label>" + fileData.FileName + "</label>");
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;
            }
        }

        var removedTemplate_Dashboard_DetailData = [];
        function Template_Dashboard_DetailDeleteRow(rowIndex, row) {
            var rowIndexTable = Template_Dashboard_DetailTable.fnGetPosition($(row).parent().parent()[0]);
            var permission = {            
                 Delete: '@permissionTemplate_Dashboard_Detail.Delete' == 'True'
            };
            if (!permission.Delete || isConsultMode)
                return;            
            if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
              if (EjecutarValidacionesAlEliminarMRTemplate_Dashboard_Detail('Template_Dashboard_Detail_', '_' + rowIndex)) {
                var prevData = Template_Dashboard_DetailTable.fnGetData(rowIndexTable );
                removedTemplate_Dashboard_DetailData.push(prevData);
                Template_Dashboard_DetailTable.fnDeleteRow(rowIndexTable );
				showTemplate_Dashboard_DetailGrid(Template_Dashboard_DetailTable.fnGetData());
              }
            }
			initiateUIControls();
        }

        function Template_Dashboard_DetailrowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionTemplate_Dashboard_Detail.Edit' == 'True',
                 Delete: '@permissionTemplate_Dashboard_Detail.Delete' == 'True'
            };
            setTemplate_Dashboard_DetailValidation();
            var Template_Dashboard_DetailRowElement = "Template_Dashboard_Detail_" + index.toString();
            $(row).addClass(Template_Dashboard_DetailRowElement);
            var nameOfTable='Template_Dashboard_Detail_';
            var rowIndex = '_' + index;
            Template_Dashboard_DetailinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertTemplate_Dashboard_DetailRowControls(index);
                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Template_Dashboard_DetailRowElement + "')){ Template_Dashboard_DetailInsertRow(" + index + "); }";
                var insertRowHTML = '<a  onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a  onclick='Template_Dashboard_DetailCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
					var idHeader = $(controls[i]).data('field') + 'Header';
					if($(controls[i]).length > 1){
						idHeader = $($(controls[i])[1]).data('field') + 'Header';
					}					

					var classe = ($('#Template_Dashboard_DetailGrid .' + idHeader).hasClass('dt-right') ? "dt-right" : "") + $('#Template_Dashboard_DetailGrid .' + idHeader ).css('display') == 'none' ? " hide" : "";
				   
					if ($(controls[i]).next().length > 0) {
