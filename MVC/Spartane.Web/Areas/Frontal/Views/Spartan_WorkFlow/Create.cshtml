        if ($('.Name' + 'Header').css('display') != 'none') columnData[counter++] = $($($.parseHTML(inputData)).val(data.Name)).addClass('Name').attr('id', nameOfTable + 'Name' + rowIndex).attr('data-field', 'Name');
