    <script type="text/javascript">
        



        $(document).ready(function () {
			var currentIndex = @ViewBag.currentRowIndex

            var Detalle_Ejercicios_RutinasRowElement = "Detalle_Ejercicios_RutinasPop_" + currentIndex.toString();
            var abc = "if(dynamicFieldValidation('" + Detalle_Ejercicios_RutinasRowElement + "')){ Detalle_Ejercicios_RutinasInsertRowFromPopup(" + currentIndex + "); }";
            var insertRowHTML = '<a class="btn btn-primary m-b" onclick="' + abc + '">Save</a>';
            $('#dvSaveJob').html(insertRowHTML);
			


            initiateUIControls();
            EjecutarValidacionesAlComienzo();
			$('a').tooltip();
        });



        function initiateUIControls() {
            $('.gridDatePicker').datepicker({
                todayBtn: "linked",
                keyboardNavigation: false,
                forceParse: false,
                calendarWeeks: true,
                autoclose: true,
                format: "@ConfigurationProperty.DateFormatDatePicker"
            });
            //Time Picker
            $('.gridTimePicker').clockpicker();

        }
		
		function SendDetalle_Ejercicios_RutinasData(callBack) {
			
					
		 var arr = []; 
		 $("form#CreateDetalle_Ejercicios_Rutinas").find(':input').each(function( index ) {		 
			if ($(this).prop('disabled')==true|| $(this).prop('disabled')=="disabled")
			{		
				arr.push($(this).attr('id')); 
				$(this).prop('disabled', false);  	
			}
	    });
		
             var formData = new FormData($("form#CreateDetalle_Ejercicios_Rutinas")[0]);
			$.each( arr, function( key, value ) {
			  $('#' +value).prop('disabled', true);
			});

            var isNew = $('#hdnIsNew').val();
           
            if (checkClientValidate('Detalle_Ejercicios_RutinasCreateForm')) {
                $.ajax({
                    url: '@Url.Content("~/Frontal/Detalle_Ejercicios_Rutinas/Post?IsNew=")' + isNew,
                    type: 'POST',
                    data: formData,
                    async: true,
                    success: function (result) {
                        if (result < 0) {
                            showNotification("@Resources.Resources.ErrorSave", "error");
                        } else {

                            showNotification("@Resources.Resources.SaveSuccess", "success");
                            callBack(result);
                        }
                    },
                    error: function (result) {
                        showNotification("@Resources.Resources.ErrorContactingServer", "error");
                    },
                    cache: false,
                    contentType: false,
                    processData: false
                });
            }
            return false;
        };
		
        function GetPropertyBag() {
            $.get('@Url.Action("Detalle_Ejercicios_RutinasPropertyBag", "Detalle_Ejercicios_Rutinas")', function (data) {
                $(".PropertyModal").html(data);
            });
        }
        function Detalle_Ejercicios_RutinasGetBusinessRule(FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null)
			{
				$('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldId').val(FieldId);
				$('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldName').val(FieldName);
				$('#hdnDetalle_Ejercicios_RutinasBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("BusinessRule", "BussinessRule")', { FieldId: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldId').val(), FieldName: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldName').val(), ObjectId: '44575', Attribute: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleAttribute').val(), Screen: 'Detalle_Ejercicios_Rutinas' }, function (data) {
                $(".Detalle_Ejercicios_RutinasBusinessRuleModal").html(data);
            });
        }
        function Detalle_Ejercicios_RutinasGetAddBusinessRule(RuleId, FieldId, FieldName, Attribute) {
			if (FieldId != null && FieldName != null) {
				$('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldId').val(FieldId);
				$('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldName').val(FieldName);
				$('#hdnDetalle_Ejercicios_RutinasBusinessRuleAttribute').val(Attribute);
			}
            $.get('@Url.Action("AddBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldId').val(), FieldName: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldName').val(), ObjectId: '44575', Attribute: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleAttribute').val(), Screen: 'Detalle_Ejercicios_Rutinas' }, function (data) {
                $(".Detalle_Ejercicios_RutinasAddBusinessRuleModal").html(data);
                $(".datatable").DataTable();
            });
        }
		function RemoveBusinessRule(RuleId) {
			$.get('@Url.Action("RemoveBusinessRule", "BussinessRule")', { BusinessRuleId: RuleId, FieldId: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldId').val(), FieldName: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleFieldName').val(), ObjectId: '44575', Attribute: $('#hdnDetalle_Ejercicios_RutinasBusinessRuleAttribute').val(), Screen: 'Detalle_Ejercicios_Rutinas' }, function (data) {
				$(".Detalle_Ejercicios_RutinasBusinessRuleModal").html(data);
			});
		}

