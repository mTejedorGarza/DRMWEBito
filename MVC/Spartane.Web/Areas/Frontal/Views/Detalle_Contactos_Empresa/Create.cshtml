		function Fill_Datos_Generales()
        {
            $.ajax({
                url: "@Url.Content("~/Frontal/Detalle_Contactos_Empresa/Get_Datos_Generales")?Id=" + $("#ReferenceFolio").val(),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                                        $('#Folio').val(result.data.Folio);
                    $('#Nombre_completo').val(result.data.Nombre_completo);
                    $('#Correo').val(result.data.Correo);
                    $('#Telefono').val(result.data.Telefono);
                    $('#Contacto_Principal').val(result.data.Contacto_Principal);
                    $('#Area').val(result.data.Area);
                    $('#Acceso_al_Sistema').val(result.data.Acceso_al_Sistema);
                    $('#Nombre_de_usuario').val(result.data.Nombre_de_usuario);
                    $('#Recibe_Alertas').val(result.data.Recibe_Alertas);
                    $('#Estatus').val(result.data.Estatus);
                    $('#Folio_Empresas').val(result.data.Folio_Empresas);

                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
		
		function Save_Datos_Generales(newId) {
			var data = new FormData();
			data.append('Folio', newId);
			                data.append('Nombre_completo', $('#Nombre_completo').val());
                data.append('Correo', $('#Correo').val());
                data.append('Telefono', $('#Telefono').val());
                data.append('Contacto_Principal', $('#Contacto_Principal').val());
                data.append('Area', $('#Area').val());
                data.append('Acceso_al_Sistema', $('#Acceso_al_Sistema').val());
                data.append('Nombre_de_usuario', $('#Nombre_de_usuario').val());
                data.append('Recibe_Alertas', $('#Recibe_Alertas').val());
                data.append('Estatus', $('#Estatus').val());
                data.append('Folio_Empresas', $('#Folio_Empresas').val());

			$.ajax({
				url: '@Url.Content("~/Frontal/Detalle_Contactos_Empresa/Post_Datos_Generales")',
				type: 'POST',
				data: data,
				async: false,
				dataType: "json",
				contentType: false,
				processData: false,
				success: function (result) {
					if (result < 0) {
						showNotification("@Resources.Resources.ErrorSave", "Error al guardar Datos_Generales");
					} else {
						newId = result;
                    
					}
				},
				error: function (result) {
					showNotification("@Resources.Resources.ErrorContactingServer", "Error al guardar Datos_Generales");
				},
				cache: false
			});
		}


