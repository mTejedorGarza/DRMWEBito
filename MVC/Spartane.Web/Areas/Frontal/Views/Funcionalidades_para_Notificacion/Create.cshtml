        function GetCampos_para_Vigencia_MS_Campos_por_FuncionalidadItem() {
            Campos_para_Vigencia_MS_Campos_por_FuncionalidadItems = null;
            var ajaxUrl = "@Url.Content("~/Frontal/Funcionalidades_para_Notificacion/GetCampos_para_Vigencia_MS_Campos_por_FuncionalidadAll")";
            $.ajax({
                url: ajaxUrl,
                type: 'GET',
