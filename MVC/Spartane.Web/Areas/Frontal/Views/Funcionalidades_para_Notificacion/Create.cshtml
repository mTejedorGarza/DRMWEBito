var MS_Campos_por_FuncionalidadData = [];
function getMS_Campos_por_FuncionalidadData() {
	$.ajax({
		url: "@Url.Content("~/Frontal/Funcionalidades_para_Notificacion/GetMS_Campos_por_Funcionalidad")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceFolio").val() != "" ? "&RelationId=" + $("#ReferenceFolio").val() : ""),
		type: 'GET',
		dataType: "json",
		async: false,
		success: function (result) {
			MS_Campos_por_FuncionalidadDataDataTable = result.data;
			$("#ddlCampos_para_VigenciaMult").val(result.data);
			$("#ddlCampos_para_VigenciaMult").trigger('chosen:updated');
		},
		error: function (result) {
			showNotification("@Resources.Resources.ErrorContactingServer", "error");
		}
	});
}

function FillCampos_para_Vigencia() {
try {
	for (var i = 0; i < Campos_para_Vigencia_MS_Campos_por_FuncionalidadItems.length ; i++) {
 
		var d = Campos_para_Vigencia_MS_Campos_por_FuncionalidadItems[i]
                $("#ddlCampos_para_VigenciaMult").append(new Option(
		d.Descripcion, d.Clave
));
	}
	$(".chosen-select").chosen({ width: "100%", no_results_text: "Oops, nothing found!" });
	$('#ddlCampos_para_VigenciaMult').children("selected").each(function () {
		$(this).prop("selected", false);
	});

	if (MS_Campos_por_FuncionalidadDataDataTable != undefined)
	{
		if (MS_Campos_por_FuncionalidadDataDataTable.length > 0)
		{
			var array = [];
			for (var j = 0; j < MS_Campos_por_FuncionalidadDataDataTable.length; j++)
			{
                               
				array.push(MS_Campos_por_FuncionalidadDataDataTable[j].Campo);


			}
			$('#ddlCampos_para_VigenciaMult').val(array);
		}  
	}
	$('#ddlCampos_para_VigenciaMult').trigger('chosen:updated');
}
catch (err) {
	alert(err.message);
}
}
