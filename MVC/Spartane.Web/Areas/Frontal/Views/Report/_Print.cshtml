@model  Spartane.Web.Areas.Frontal.Models.PrintReport

@{

    var tabla = Spartane.Web.Helpers.DataSetConverter.ConvertDataTableToHTML(@Model.Data);
}
<head>
<title>@Model.Report_Name</title>
</head>
<style>
    .table {
        width: 100%;
        border: 1px solid #cccccc;
        font-family: "open sans","Helvetica Neue",Helvetica,Arial,sans-serif;
        /*border-collapse: separate !important;*/
        font-size: 13px;
    }

        .table td {
            padding: 2px;
            border:1px solid #cccccc;
        }

    .table tr:nth-of-type(2n+1) {
        background-color: #f9f9f9;
    }

        .table tr:nth-of-type(2n) {
            background-color: #8da0c2;
        }

    .table-row-1 td, .table-row-1 th {
    }

    .table-row-2 td, .table-row-2 th, thead {
    }

    .tableHeaderRow {
        font-weight: bold;
        border: 1px solid #ff6a00;
    }

    .column1 {
        width: 15%;
    }

    .column2 {
        width: 15%;
    }
</style>
<br />
<center><h3>@Model.Report_Name</h3></center>
<br />

 @Html.Raw(@tabla);


