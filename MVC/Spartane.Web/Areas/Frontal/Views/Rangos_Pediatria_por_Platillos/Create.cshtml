			 if (viewInEframe) {             
                $('#Rangos_Pediatria_por_PlatillosGuardarYNuevo').hide();
                $('#Rangos_Pediatria_por_PlatillosGuardarYCopia').hide();
                $('#Rangos_Pediatria_por_PlatillosCancelar').hide(); 
