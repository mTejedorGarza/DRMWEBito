            }

			
			

        });

        var MR_PadecimientosinsertRowCurrentIndex = -1;
        var MR_PadecimientosDataDataTable;
        function getMR_PadecimientosData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/Rangos_Pediatria_por_Platillos/GetMR_Padecimientos")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceFolio").val() != "" ? "&RelationId=" + $("#ReferenceFolio").val() : ""),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                    MR_PadecimientosDataDataTable = result.data;
                    showMR_PadecimientosGrid(result.data);
                    $('#MR_PadecimientosGrid .inputMoney').inputmask("currency", { prefix: "$", rightAlign: false });
                    $('#MR_PadecimientosGrid .gridDatePicker').attr("readonly", "");
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
        function MR_PadecimientosGetUpdateRowControls(data, nameOfTable, rowIndex) {

            var columnData = [];
            var counter = 0;
            var inputData = "<input type='text' class='fullWidth form-control' />";

           columnData[counter++] = $($(GetMR_Padecimientos_PadecimientosDropDown()).val(data.Padecimiento)).addClass('Padecimiento inputNumber').attr('id', nameOfTable + 'Padecimiento' + rowIndex).attr('data-field', 'Padecimiento').after($.parseHTML(addNew('MR_Padecimientos', 'Padecimientos','Padecimiento', 261317)));

            return columnData;
        }



        function GetFileInfo(fileData, fileUploadData) {

            var $div = $("<div>");
            if (fileUploadData != null && fileUploadData.files.length > 0) {

                $div.append("<label>" + fileUploadData.files[0].name + "</label>");
                //<i class="fa fa-times-circle" id='removeFileAttachment'>
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;

            } else if (fileUploadData != null) {
                return $.parseHTML(GetFileUploader());
            } else {
                $div.append("<label>" + fileData.FileName + "</label>");
                $div.append($("<i class='fa fa-times-circle' id='removeFileAttachment'>").click(function () {
                    if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
                        var parent = $(this).parent().parent();
                        parent.empty();
                        parent.append("<input type='file'>");
                    }
                }));
                return $div;
            }
        }

        var removedMR_PadecimientosData = [];
        function MR_PadecimientosDeleteRow(rowIndex, row) {
            var rowIndexTable = MR_PadecimientosTable.fnGetPosition($(row).parent().parent()[0]);
            var permission = {            
                 Delete: '@permissionMR_Padecimientos.Delete' == 'True'
            };
            if (!permission.Delete || isConsultMode)
                return;            
            if (confirm(htmlDecode("@Resources.Resources.DeleteConfirm"))) {
              if (EjecutarValidacionesAlEliminarMRMR_Padecimientos('MR_Padecimientos_', '_' + rowIndex)) {
                var prevData = MR_PadecimientosTable.fnGetData(rowIndexTable );
                removedMR_PadecimientosData.push(prevData);
                MR_PadecimientosTable.fnDeleteRow(rowIndexTable );
				showMR_PadecimientosGrid(MR_PadecimientosTable.fnGetData());
              }
            }
			initiateUIControls();
        }

        function MR_PadecimientosrowCreationGrid(row, data, index) {
            var permission =  {            
                 Edit: '@permissionMR_Padecimientos.Edit' == 'True',
                 Delete: '@permissionMR_Padecimientos.Delete' == 'True'
            };
            setMR_PadecimientosValidation();
            var MR_PadecimientosRowElement = "MR_Padecimientos_" + index.toString();
            $(row).addClass(MR_PadecimientosRowElement);
            var nameOfTable='MR_Padecimientos_';
            var rowIndex = '_' + index;
            MR_PadecimientosinsertRowCurrentIndex = index;

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
