        var Spartan_BR_Action_Configuration_DetailinsertRowCurrentIndex = -1;
        var Spartan_BR_Action_Configuration_DetailDataDataTable;
        function getSpartan_BR_Action_Configuration_DetailData() {
            $.ajax({
                url: "@Url.Content("~/Frontal/Spartan_BR_Action/GetSpartan_BR_Action_Configuration_Detail")" + "?draw=1&start=1&length=2147483647" + ($("#ReferenceActionId").val() != "" ? "&RelationId=" + $("#ReferenceActionId").val() : ""),
                type: 'GET',
                dataType: "json",
                async: false,
                success: function (result) {
                    Spartan_BR_Action_Configuration_DetailDataDataTable = result.data;
                    showSpartan_BR_Action_Configuration_DetailGrid(result.data);
                },
                error: function (result) {
                    showNotification("@Resources.Resources.ErrorContactingServer", "error");
                }
            });
        }
        function Spartan_BR_Action_Configuration_DetailGetUpdateRowControls(data) {

            var columnData = [];
            var inputData = "<input type='text' class='fullWidth form-control' />";
            columnData[0] = $($($.parseHTML(inputData)).val(data.Parameter_Name)).addClass('Parameter_Name');
            columnData[1] = $($(GetSpartan_BR_Action_Configuration_Detail_Spartan_BR_Action_Param_TypeDropDown()).val(data.Parameter_Type)).addClass('Parameter_Type');

            return columnData;
        }

        var removedSpartan_BR_Action_Configuration_DetailData = [];
        function Spartan_BR_Action_Configuration_DetailDeleteRow(rowIndex) {
            if (!permission.Delete || isConsultMode)
                return;

            if (confirm("@Resources.Resources.DeleteConfirm")) {
                var prevData = Spartan_BR_Action_Configuration_DetailTable.fnGetData(rowIndex);
                removedSpartan_BR_Action_Configuration_DetailData.push(prevData);
                Spartan_BR_Action_Configuration_DetailTable.fnDeleteRow(rowIndex);
            }

        }

        function Spartan_BR_Action_Configuration_DetailrowCreationGrid(row, data, index) {
            //debugger;
            var Spartan_BR_Action_Configuration_DetailRowElement = "Spartan_BR_Action_Configuration_Detail_" + index.toString();
            $(row).addClass(Spartan_BR_Action_Configuration_DetailRowElement);

            row.innerHTML = "";

            if (data.IsInsertRow) {

                if (row.childNodes.length != 0) {
                    for (var i = 0; i < row.childNodes.length; i++) {
                        row.childNodes[i].remove();
                    }
                }
                var controls = GetInsertSpartan_BR_Action_Configuration_DetailRowControls(index);

                var actionColInsert = $('<td>');

                var abc = "if(dynamicFieldValidation('" + Spartan_BR_Action_Configuration_DetailRowElement + "')){ Spartan_BR_Action_Configuration_DetailInsertRow(" + index + "); }";
                var insertRowHTML = '<a  onclick="' + abc + '">';
                $('<i class="fa fa-check">').appendTo($(insertRowHTML).appendTo(actionColInsert));
				$('<i class="fa fa-times">').appendTo($("<a  onclick='Spartan_BR_Action_Configuration_DetailCancelRow(" + index + ")'>").appendTo(actionColInsert));
                actionColInsert.appendTo(row);

                for (i = 0; i < controls.length; i++) {
                    $(controls[i]).appendTo($('<td>').appendTo(row));
                }
                return;
            }

            var actionCol = $('<td>');
            $('<i class="fa fa-pencil">').appendTo($("<a title='Edit' class='" + (!permission.Edit || isConsultMode ? " disabled" : "") + "' onclick='Spartan_BR_Action_Configuration_DetailEditRow(" + index + ")'>").appendTo(actionCol));
            @*$('<i class="fa fa-edit">').appendTo($("<a title='Edit' data-toggle='modal' data-target='#AddSpartan_BR_Action_Configuration_Detail-form' class='" + (!permission.Edit || isConsultMode ? " disabled" : "") + "' onclick='Spartan_BR_Action_Configuration_DetailEditRowPopup(" + index + ")'>").appendTo(actionCol));*@
            $('<i class="fa fa-trash-o">').appendTo($("<a title='Delete' class='" + (!permission.Delete || isConsultMode ? " disabled" : "") + "' onclick='Spartan_BR_Action_Configuration_DetailDeleteRow(" + index + ")'>").appendTo(actionCol));
            actionCol.appendTo(row);
            $('<td>').text(data.Parameter_Name).appendTo(row);
            $('<td>').text(GetSpartan_BR_Action_Configuration_Detail_Spartan_BR_Action_Param_TypeName(data.Parameter_Type)).appendTo(row);

        }

