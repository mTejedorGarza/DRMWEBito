    <script type="text/javascript">
        



        $(document).ready(function () {


            GetSpartan_BR_Action_Configuration_Detail_Spartan_BR_Action_Param_TypeItem();

            getSpartan_BR_Action_Configuration_DetailData();
            GetSpartan_BR_Attribute_Restrictions_Detail_Spartan_Attribute_TypeItem();
