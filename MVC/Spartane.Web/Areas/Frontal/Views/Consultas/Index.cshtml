        + "&Frecuencia_cardiaca_en_Esfuerzo=" + Frecuencia_cardiaca_en_Esfuerzo
        + "&Interpretacion_frecuencia=" + Interpretacion_frecuencia
        + "&Indice_cintura_cadera=" + Indice_cintura_cadera
